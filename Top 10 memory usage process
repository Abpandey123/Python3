from subprocess import check_output as cmd
command = 'ps -A --sort pmem -o pid,comm,pmem'
out = cmd(command.split()).decode().splitlines()
header = out[0]
mem_usage = [i.split() for i in out[2:]][-11:]
print('Top 10 Memory consumer:')
print('*'*30)
for j in mem_usage:
    print('{:7} {:18} {}'.format(*j))
